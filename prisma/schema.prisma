datasource db {
  provider = "mysql"
  url      = env("PLANETSCALE_DB")
  referentialIntegrity = "prisma"
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

model Recipe {
  id              Int       @default(autoincrement()) @id
  title            String    @db.VarChar(255)
  notes   String    @db.VarChar(255)
}